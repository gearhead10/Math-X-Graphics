mixin symbols()
  .modal-content
    h1 Tabla de símbolos disponibles
    .row
      .col.s12
        table.table
          thead
            tr
              th(scope="col") Símbolo
              th(scope="col") Significado
          tbody
            tr
              td ∅ - 0 - Ø
              td Conjunto vacío
            tr
              td Ω - O
              td Universo
            tr
              td S - s
              td Ω - AUBU...
            tr
              td :dog:
              td 🐕
            tr
              td :fox:
              td 🦊
            tr
              td :lion:
              td 🦁
            tr
              td :horse:
              td 🐴
            tr
              td :cow:
              td 🐄
            tr
              td :pig:
              td 🐖
            tr
              td :elephant:
              td 🐘
            tr
              td :squirrel:
              td 🐿
            tr
              td :chicken:
              td 🐔
            tr
              td :dino:
              td 🦕
            tr
              td :egg:
              td 🥚
            
  .modal-footer
    a.modal-close.waves-effect.btn-flat Aceptar